{
	"name": "typescript-imports-sort",
	"displayName": "Typescript Imports Sort",
	"description": "Very specific typescript import sorter",
	"version": "0.0.1",
	"license": "MIT",
	"categories": [
		"Formatters"
	],
	"engines": {
		"vscode": "^1.80.0"
	},
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.sortYmports",
				"title": "Sort imports"
			}
		]
	},
	"keywords": [
		"Typescript",
		"imports",
		"sort",
		"import statements",
		"import clauses"
	],
	"scripts": {
		"dev": "ts-node src/index.ts",
		"test": "vitest run",
		"compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.26.0",
		"lodash": "^4.17.21",
		"tslint": "^5.11.0",
		"typescript": "^5.3.2"
	},
	"dependencies": {
		"ts-node": "^10.9.2",
		"vitest": "^1.2.2"
	}
}
